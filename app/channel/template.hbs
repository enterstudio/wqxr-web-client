{{#ember-wormhole to="js-listings"}}
  {{#google-ad ad="leaderboard" hasMarquee=model.hasMarquee class="l-constrained"}}
    <div id="leaderboard" class="aligncenter"></div>
  {{/google-ad}}

  {{#if model.hasMarquee}}
    {{x-marquee model=model}}
  {{else}}
    {{#if model.hasFeatured}}
      {{channel-header model=model}}
    {{else}}
      {{channel-header model=model classNames='flush'}}
    {{/if}}
  {{/if}}

  <div class="l-constrained">        
    <main class="l-col2of3">

      {{#if model.hasFeatured}}
      <section>
        <h1 class="for-screenreaders">Featured Item</h1>
        {{story-tease
          item=model.featured
          hideLinks=true
          isFeatured=true
          class='box box--bgoverflow'}}
      </section>
      {{/if}}

      {{#if model.linkroll}}
        {{nav-links channelType=channelType links=model.linkroll action="updateRouteTitle"}}
      {{/if}}

      {{outlet}}

    </main>

    <aside class="l-col1of3">
      <div class="l-skinny">

        {{#if model.tease}}
        <div class="box box--nopad">
          <div class="box-heading">
            <h1 class="h5 text--gray">About the {{capitalize-word model.listingObjectType}}</h1>
          </div>
          <div class="text text--medium">
            <div class="text-body text-body--nopad text-body--forceinline">
              {{{model.tease}}}
              {{#if model.producingOrganizations}}
                {{producing-orgs model.producingOrganizations}}
              {{/if}}
            </div>
          </div>
        </div>
        <!-- .box -->
        {{/if}}


        {{social-links model=model}}

        {{{model.chunkSidebarTop}}}

        {{google-ad ad="rightRail" class="align--mq-centertoright channel-ad"}}

        {{{model.chunkSidebarBottom}}}

      </div>
      <!-- .l-skinny -->
    </aside>
  </div>
{{/ember-wormhole}}
