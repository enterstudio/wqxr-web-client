{{!-- nypr-brick-wrapper/template.hbs
groups = [ [item, item, item], [item, item, item], [item, item] ]
--}}
{{#each groups as |group index|}}
  {{#if (eq index 2)}}
  {{brick-card-row items=group}}
  <!-- an-ad component -->
  {{else}}
  {{brick-image-row items=group flip=(eq index 1)}}

  {{/if}}
{{/each}}
