<ul>
  {{#each largeStoryRow1 as |story index|}}
  <li class="{{story.attributes.template}} story--trending"
      style="background-image: url({{story.attributes.imageMain.url}})">
    <div class="story__icons">
      <div class="story__icons__trending icon--with-text icon--with-text--top">
        {{nypr-svg className="icon" icon="trending"}} Trending
      </div>
      <div class="story__icons__video">
        {{nypr-svg className="icon" icon="video"}}
      </div>
    </div>
      <div class="story__head">
        <a class="story__head__brand" href="{{story.attributes.headers.brand.url}}">
          {{story.attributes.headers.brand.title}}
        </a>
        <h2 class="story__head__headline"><a href="{{story.attributes.url}}">
          {{story.attributes.title}}
        </a></h2>
      </div>
  </li>
  {{/each}}
